{% extends "layout.jinja2" %}

{% block body %}
    <main>
        <div class="well1">
        <h3>Edit your Entry</h3>
            <article class="edit">
                <form method="POST" action="" enctype="multipart/form-data"> 
                <h4>Title - Week:</h4>
                    <input type="text" name="title" value="{{ entry.title }}">
                <h4>Title - Day:</h4>
                    <input type="text" name="title1" value="{{ entry.title1 }}">
                <h4>Body:</h4>
                    <textarea name="entry_body">{{ entry.body }}</textarea>
                    <input class="btn" type="submit" value="Submit">
                </form>
            </article>
        </div>
    </main>
{% endblock %}